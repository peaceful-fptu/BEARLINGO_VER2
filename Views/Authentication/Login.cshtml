    <main class="p-v-5-m">
        <div class="container m-auto-xs col-4-l col-5-m col-10-xs p-h-6-l">
            <h3 class="heading3 text-center-xs">Log in</h3>
            <form class="recaptcha-protected password-form" asp-action="Login" asp-controller="Login" accept-charset="UTF-8" method="post">
                <input name="utf8" type="hidden" value="&#x2713;" /><input type="hidden" name="authenticity_token" value="wLNDCnHSYChcO/mcXFS2/Z3GuYtYMvFAWezXokBbnKNIJx9dJ65kPZbALwjXpSmYc/b31WnKOocZvQXN9ZVMYw==" />
                <div class="m-b-4-xs">
                    <label class="form-label bodyMain p-b-0-xs m-b-2-xs" for="email">Email</label>
                    <input type="email" name="email" id="email" required="required" autocomplete="email" class="form-input p-v-1-xs p-l-2-xs p-r-4-xs col-12-xs bodyMain" />
                    <div>@ViewBag.messageErrorEmail</div>
                </div>
                <div class="m-b-4-xs">
                    <label class="form-label bodyMain p-b-0-xs m-b-2-xs" for="password">Password</label>
                    <div class="password-container">
                        <input type="password" name="password" id="password" value="" required="required" autocomplete="current-password" class="password form-input p-v-1-xs p-l-2-xs p-r-6-xs col-12-xs bodyMain" />
                        <svg width="16" height="16" class="icon-e" id="icon-e">
                            <use xlink:href="#icon__Eye"></use>
                        </svg>
                        <div>@ViewBag.messageErrorPassword</div>
                    </div>
                </div>
                <div class="dsp-flex-xs m-b-1-xs flex-justify-content-space-between-xs">
                    <div>
                        <a class="bodySmall form-footer" asp-area="" asp-controller="ForgotPassword" asp-action="ForgotPassword">Forgot Password</a>
                    </div>
                    <div>
                        <a class="bodySmall form-footer" asp-area="" asp-controller="Register" asp-action="Index">No Account</a>
                    </div>
                </div>
                <div class="dsp-flex-xs flex-justify-content-center-xs">
                    <input type="submit" name="commit" value="Log in" class="btn-primary button p-v-2-xs p-h-5-xs m-v-3-xs col-12-xs" data-testid="login-button" data-disable-with="Log in" />
                </div>
                <div class="g-recaptcha-v3" data-action="login_check_password" data-sitekey-v3="6Le9jOUUAAAAAMKU_R7rTbi4JFAi6FXIhG5aBRHx"></div>
            </form>
            <hr class="col-12-xs m-v-3-xs" />
            <div class="dsp-flex-xs flex-justify-content-center-xs">
                <a class="button oauth-button col-12-xs m-v-3-xs dsp-flex-xs flex-align-items-center-xs flex-justify-content-center-xs" data-test-id="login-with-google" onclick="openWindowWithParameters(550, 550, this.getAttribute(&#39;href&#39;), &#39;google&#39;); return false;"
                   asp-action="OnPost" asp-controller="GoogleLogin">
                    <div class="col-3-xs dsp-flex-xs flex-align-items-center-xs flex-justify-content-center-xs">
